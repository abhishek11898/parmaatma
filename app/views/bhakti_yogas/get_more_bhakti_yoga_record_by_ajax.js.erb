<% if @bhakti_yogas.present? %>
  var $container = '<%= j(render partial: "card", locals: { bhakti_yogas: @bhakti_yogas, total_records: @total_records}) %>';
  var row_1_childs = jQuery($container).find('.row-1').children();
  var row_2_childs = jQuery($container).find('.row-2').children();
  var row_3_childs = jQuery($container).find('.row-3').children();
  jQuery('.yoga-marg-container')find('.row-1').append(row_1_childs);
  jQuery('.yoga-marg-container')find('.row-2').append(row_2_childs);
  jQuery('.yoga-marg-container')find('.row-3').append(row_3_childs);
<% else %>
  alert('No more records available');
<% end %>

